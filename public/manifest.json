{
  "name": "bookmarks & tabs",
  "version": "1.0",
  "manifest_version": 3,

  "action": {
    "default_title": "open manager"
  },
  "background": {
    "service_worker": "background.js"
  },
  "chrome_url_overrides": {
    "bookmarks": "index.html"
  },
  "permissions": [
    "bookmarks",
    "tabs"
  ]
}
