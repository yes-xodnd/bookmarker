{
  "name": "bookmark manager",
  "version": "0.1",
  "manifest_version": 3,

  "action": {
    "default_title": "open bookmark manager"
  },
  "background": {
    "service_worker": "background.js"
  },
  "chrome_url_overrides": {
    "bookmarks": "index.html"
  },
  "permissions": [
    "bookmarks",
    "tabs"
  ]
}
